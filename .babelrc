{
  "presets": [
    ["env", {
      "targets": { "node": 6 },
      "useBuiltIns": true
    }],
    "stage-0",
    "react"
  ],
  "plugins": ["add-module-exports", "dynamic-import-webpack"],
  "env": {
    "development": {
      "plugins": [
        "transform-class-properties",
        "transform-es2015-classes",
        "react-hot-loader/babel"
      ]
    }
  }
}
