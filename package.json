{
  "name": "spendless",
  "version": "0.1.0",
  "description": "Personal finance tracker built using electron",
  "main": "main.js",
  "scripts": {
    "db-server": "pouchdb-server -d data/",
    "hot-updates-server": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.config.babel.js",
    "start": "cross-env NODE_ENV=production electron ./",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./main",
    "test": "cross-env NODE_ENV=test nyc mocha --reporter spec tools/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "coverage": "nyc report",
    "dev": "cross-env START_HOT=1 npm run hot-updates-server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joeynguyen/spendless.git"
  },
  "author": "Giao Nguyen",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/joeynguyen/spendless/issues"
  },
  "keywords": [
    "babel",
    "bootstrap",
    "electron",
    "es6",
    "express",
    "node",
    "pouchdb",
    "react",
    "react-bootstrap",
    "react-router",
    "redux",
    "webpack"
  ],
  "homepage": "https://github.com/joeynguyen/spendless",
