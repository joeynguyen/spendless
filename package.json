{
  "name": "spendless",
  "version": "0.3.0",
  "description": "Personal finance tracker built using electron",
  "main": "main.js",
  "scripts": {
    "db-server": "pouchdb-server -d data/",
    "coverage": "nyc report",
    "start": "concurrently 'yarn react-start' 'wait-on http://localhost:3000/ && yarn electron-start'",
    "build": "yarn react-build && yarn electron-build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "react-start": "BROWSER=none react-scripts start",
    "electron-start": "ELECTRON_IS_DEV=1 electron .",
    "react-build": "react-scripts build",
    "electron-build": "electron-packager ./ --overwrite"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joeynguyen/spendless.git"
  },
  "author": "Giao Nguyen",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/joeynguyen/spendless/issues"
  },
  "keywords": [
    "antd",
    "electron",
    "es6",
    "node",
    "pouchdb",
    "react",
    "react-router",
    "redux"
  ],
  "homepage": "https://github.com/joeynguyen/spendless",
  "devDependencies": {
    "concurrently": "3.5.1",
    "electron": "1.8.3",
    "electron-devtools-installer": "2.2.3",
    "electron-packager": "11.1.0",
    "nyc": "8.3.0",
    "pouchdb-server": "2.3.7",
    "react-scripts": "1.1.1",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-logger": "3.0.6",
    "wait-on": "2.1.0"
  },
  "dependencies": {
    "antd": "2.12.8",
    "font-awesome": "4.7.0",
    "history": "4.6.1",
    "moment": "2.12.0",
    "pouchdb-browser": "6.1.2",
    "pouchdb-find": "0.10.5",
    "prop-types": "15.5.10",
    "react": "^16.2.0",
    "react-collapse": "4.0.2",
    "react-dom": "^16.2.0",
    "react-file-reader": "1.1.2",
    "react-motion": "0.5.0",
    "react-redux": "5.0.5",
    "react-router": "4.1.1",
    "react-router-dom": "4.1.1",
    "react-router-redux": "5.0.0-alpha.6",
    "redux": "3.5.2",
    "redux-thunk": "2.0.1"
  }
}
