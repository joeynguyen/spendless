{
  "name": "spendless",
  "version": "0.3.0",
  "description": "Personal finance tracker built using electron",
  "main": "main.js",
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "db-server": "pouchdb-server -d data/",
    "coverage": "nyc report",
    "start": "concurrently 'yarn react-start' 'wait-on http://localhost:3000/ && yarn electron-start'",
    "build": "yarn react-build && yarn electron-build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "react-start": "BROWSER=none react-scripts-ts start",
    "electron-ts-compile": "tsc --lib 'es2015, dom' main.ts",
    "electron-start": "yarn electron-ts-compile && ELECTRON_IS_DEV=1 electron .",
    "react-build": "react-scripts-ts build",
    "electron-build": "yarn electron-ts-compile && electron-packager ./ --overwrite",
    "prettier": "prettier --write './src/**/*.{ts,js,css,json}'",
    "precommit": "pretty-quick --staged"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joeynguyen/spendless.git"
  },
  "author": "Giao Nguyen",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/joeynguyen/spendless/issues"
  },
  "keywords": [
    "antd",
    "electron",
    "es6",
    "node",
    "pouchdb",
    "prettier",
    "react",
    "react-router",
    "redux",
    "typescript"
  ],
  "homepage": "./",
  "devDependencies": {
    "@types/jest": "23.1.1",
    "@types/react": "16.4.1",
    "@types/react-dom": "16.0.6",
    "concurrently": "3.5.1",
    "electron": "1.8.7",
    "electron-devtools-installer": "2.2.3",
    "electron-packager": "11.1.0",
    "husky": "0.14.3",
    "nyc": "8.3.0",
    "pouchdb-server": "2.3.7",
    "prettier": "1.13.0",
    "pretty-quick": "1.6.0",
    "react-perf-devtool": "3.0.7",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-logger": "3.0.6",
    "source-map-explorer": "1.5.0",
    "typescript": "2.9.2",
    "wait-on": "2.1.0"
  },
  "dependencies": {
    "antd": "3.6.4",
    "font-awesome": "4.7.0",
    "history": "4.6.1",
    "moment": "2.12.0",
    "pouchdb-browser": "6.1.2",
    "pouchdb-find": "0.10.5",
    "prop-types": "15.5.10",
    "react": "16.4.1",
    "react-collapse": "4.0.3",
    "react-dom": "16.4.1",
    "react-file-reader": "1.1.2",
    "react-motion": "0.5.2",
    "react-redux": "5.0.5",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-router-redux": "5.0.0-alpha.9",
    "react-scripts-ts": "2.16.0",
    "redux": "3.5.2",
    "redux-thunk": "2.0.1"
  }
}
